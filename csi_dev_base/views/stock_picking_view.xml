<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="stock_picking_barcode_extend" model="ir.ui.view">
	        <field name="name">stock.picking.form.view.barcode.extend</field>
        	<field name="model">stock.picking</field>
	        <field name="inherit_id" ref="stock_barcode.stock_picking_barcode" />
	        <field name="arch" type="xml">
	            <xpath expr="//field[@name='move_lines']/tree/field[@name='product_uom_qty']" position="after">
	            	<field name="quantity_done"/>
	            </xpath>
	            <xpath expr="//field[@name='move_lines']" position="after">
					<field name="custom_move_lines" attrs="{'invisible': [('custom_move_lines', '=', False)]}" >
                        <tree>
							<field name="product_id"/>
							<field name="product_uom_qty_initial"/>
							<field name="product_uom_qty"/>
						</tree>
                    </field>
	            </xpath>
	            <field name="move_lines" position="before">
					<field name="custom_quality_alert" widget="many2many_tags" readonly="1"/>
				</field>
	        </field>
	    </record>
	    
	    <record id="view_picking_form_extend" model="ir.ui.view">
			<field name="name">stock.picking.form.extend</field>
			<field name="model">stock.picking</field>
			<field name="inherit_id" ref="stock.view_picking_form" />
			<field name="arch" type="xml">
				<field name="picking_type_id" position="after">
					<field name="custom_quality_alert" widget="many2many_tags_link" readonly="1"/>
				</field>
			</field>
		</record>
	</data>
</odoo>